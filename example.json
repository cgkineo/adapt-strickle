// To go in the contentObject.json file
//enables trickle on the page
"_strickle": {
    "_isEnabled": true,
    "_autoScroll": true,
    "_bottomPadding": 20,
    "_animateSpeed": 1000,
    "_waitForEvent": "tutor:closed",
    "_ignoreComponents": [
        "blank",
        "text"
    ]
},


//To got in the component.json file
//skips trickle on the component
"_strickle": {
    "_isEnabled": false
}
